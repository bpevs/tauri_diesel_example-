{
  "$schema": "./schemas/android-schema.json",
  "identifier": "default-plugins",
  "description": "enables the default permissions",
  "windows": ["main"],
  "permissions": [
    "todos-db:allow-add-todo",
    "todos-db:allow-list-todos",
    "path:default",
    "event:default",
    "window:default",
    "webview:default",
    "app:default",
    "resources:default",
    "menu:default",
    "tray:default"
  ]
}
